# SI-NSA SRP Environment Variables Template
# Copy this file to .env and fill in your values

# App Configuration
REACT_APP_NAME=SI-NSA SRP
REACT_APP_VERSION=1.0.0

# API Configuration (for future use)
# REACT_APP_API_URL=https://api.example.com
# REACT_APP_API_KEY=your_api_key_here

# Storage Configuration
REACT_APP_STORAGE_PREFIX=sinsa_srp_

# Feature Flags (for future use)
# REACT_APP_ENABLE_REPORTS=true
# REACT_APP_ENABLE_EXPORT=true
# REACT_APP_ENABLE_SYNC=false

# Development
# REACT_APP_DEBUG=true
# REACT_APP_LOG_LEVEL=debug

# Production
# REACT_APP_DEBUG=false
# REACT_APP_LOG_LEVEL=error

# Build Configuration
# GENERATE_SOURCEMAP=false
# INLINE_RUNTIME_CHUNK=false

# Capacitor Build (Android/iOS)
# CAPACITOR_ANDROID_STUDIO_PATH=/path/to/android-studio
# CAPACITOR_XCODE_PATH=/path/to/xcode

# Electron Build
# ELECTRON_BUILD_TARGET=nsis,portable